{
	"name": "split-toolkit",
	"displayName": "Split Toolkit",
	"publisher": "xgeeks",
	"description": "SPLIT Toolkit",
	"icon": "images/icon.png",
	"version": "0.0.2",
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:split-toolkit.split-up",
		"onCommand:split-toolkit.split-down",
		"onCommand:split-toolkit.split-run"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "split-toolkit.split-up",
				"title": "SPLIT Up"
			},
			{
				"command": "split-toolkit.split-down",
				"title": "SPLIT Down"
			},
			{
				"command": "split-toolkit.split-run",
				"title": "SPLIT Run"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^8.0.0",
		"@types/mocha": "^10.0.1",
		"@types/node": "16.x",
		"@types/vscode": "^1.74.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"@vscode/test-electron": "^2.2.0",
		"eslint": "^8.28.0",
		"glob": "^8.0.3",
		"mocha": "^10.1.0",
		"typescript": "^4.9.3"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com:StereoPT/split-toolkit.git"
	}
}
